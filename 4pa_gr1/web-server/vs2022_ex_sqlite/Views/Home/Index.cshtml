@model List<Product>
@{
    ViewData["Title"] = "Home Page";
}
<table class="table table-striped">
    <thead>
        <tr>
            <th>Nazwa</th>
            <th>Cena</th>
        </tr>        
    </thead>
    <tbody>
        @foreach(var p in Model)
        {
            <tr>
                <td>
                    @p.Name
                </td>
                <td>
                    @p.Price
                </td>
            </tr>
        }
      
    </tbody>
   
</table>

